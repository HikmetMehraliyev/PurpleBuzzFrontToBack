@model List<TeamMember>


    @foreach (TeamMember members in Model)
    {
        <div class="team-member col-md-4">
            <img class="team-member-img img-fluid rounded-circle p-4" src="~/assets/img/@members.ImagePath" alt="Card image">
            <ul class="team-member-caption list-unstyled text-center pt-4 text-muted light-300">
                <li>@members.FullName</li>
                <li>@members.Profession</li>
            </ul>
        </div>
    }

